#
# Maven aliases
#

# Clean (c)
alias mc='mvn clean'

# Build (b)  
alias mb='mvn -DskipTests=true install'
alias mbp='mvn -DskipTests=true install --projects'
alias mcb='mvn -DskipTests=true clean install'
alias mcbp='mvn -DskipTests=true clean install --projects'

# Test (t)
alias mt='mvn install'
alias mtp='mvn install --projects'
alias mct='mvn clean install'
alias mctp='mvn clean install --projects'
