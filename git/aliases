#
# Git aliases
#

# Commit (c)
alias gcx='git cherry-pick --abort'
alias gcm='git commit --all --message'

# Index (i)
alias giaa='git add --all'

# Pull request (pr)
alias gprh='~/.dotfiles/git/git-pull-request-handle'

# Push (p)
alias gpo='git push origin'
alias gpom='git push origin master'

# Topic Banch (tb)
alias gtbX='~/.dotfiles/git/git-branch-remove-list'

# Update (u)
alias gu='~/.dotfiles/git/git-update'

# Sync (y)
alias gyu='~/.dotfiles/git/git-sync-upstream'